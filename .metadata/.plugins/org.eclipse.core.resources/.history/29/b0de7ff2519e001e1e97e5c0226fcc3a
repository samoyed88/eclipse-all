import java.util.*;
public class test_1212 {

	public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		star(sc.nextInt());
		System.out.println();
		System.out.println("Knowledge is power!\r\n");
		System.out.println();
		star(sc.nextInt());
		
	}
	public static void star(int c) {
		for(int i=0;i<=c;i++) {
			System.out.print("*");
		}
	}
	

}
